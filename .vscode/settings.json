{
    "robot.variables": {
        "ENV_NAME": "local"
    },
    "robot.pythonpath": [
        "${workspaceFolder}/lib"
    ],
    "robot.libraries.libdoc.preGenerate": [
        "${workspaceFolder}/lib/ApplicationLibrary",
        "${workspaceFolder}/lib/PostgreSQLLibrary",
        "${workspaceFolder}/lib/FakeItLibrary",
    ],
    "robot.codeFormatter": "robotidy",
    "robot.completions.keywords.prefixImportName": false,
    "robot.lint.enabled": true,
    "robot.lint.unusedKeyword": true,
    "robot.lint.ignoreVariables": [],
    "robot.interactiveConsole.arguments": [
        "--output",
        "${workspaceFolder}/log/robot_ic.xml"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
}
